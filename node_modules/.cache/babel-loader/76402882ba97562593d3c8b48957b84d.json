= false\n    const activated = matched.filter((c, i) => {\n      return diffed || (diffed = (prevMatched[i] !== c))\n    })\n\n    if (!activated.length) return next()\n\n    // 这里如果有加载指示器 (loading indicator)，就触发\n\n    Promise.all(activated.map(c => {\n      if (c.asyncData) {\n        return c.asyncData({ store, route: to })\n      }\n    })).then(() => {\n      // 停止加载指示器(loading indicator)\n      next()\n    }).catch(next)\n  })\n  app.$mount('#app', true)\n})\n"],"mappings":";;;;;;;;;AAAA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA,SAASA,SAAS,QAAQ,QAAQ;AAAA,iBAEHA,SAAS,EAAE;EAAlCC,GAAG,cAAHA,GAAG;EAAEC,MAAM,cAANA,MAAM;EAAEC,KAAK,cAALA,KAAK;AAE1B,IAAIC,MAAM,CAACC,iBAAiB,EAAE;EAC5BF,KAAK,CAACG,YAAY,CAACF,MAAM,CAACC,iBAAiB,CAAC;AAC9C;AAEAH,MAAM,CAACK,OAAO,CAAC,YAAM;EACnB;EACA;EACA;EACA;EACAL,MAAM,CAACM,aAAa,CAAC,UAACC,EAAE,EAAEC,IAAI,EAAEC,IAAI,EAAK;IACvC,IAAMC,OAAO,GAAGV,MAAM,CAACW,oBAAoB,CAACJ,EAAE,CAAC;IAC/C,IAAMK,WAAW,GAAGZ,MAAM,CAACW,oBAAoB,CAACH,IAAI,CAAC;;IAErD;IACA;IACA,IAAIK,MAAM,GAAG,KAAK;IAClB,IAAMC,SAAS,GAAGJ,OAAO,CAACK,MAAM,CAAC,UAACC,CAAC,EAAEC,CAAC,EAAK;MACzC,OAAOJ,MAAM,KAAKA,MAAM,GAAID,WAAW,CAACK,CAAC,CAAC,KAAKD,CAAE,CAAC;IACpD,CAAC,CAAC;IAEF,IAAI,CAACF,SAAS,CAACI,MAAM,EAAE,OAAOT,IAAI,EAAE;;IAEpC;;IAEAU,OAAO,CAACC,GAAG,CAACN,SAAS,CAACO,GAAG,CAAC,UAAAL,CAAC,EAAI;MAC7B,IAAIA,CAAC,CAACM,SAAS,EAAE;QACf,OAAON,CAAC,CAACM,SAAS,CAAC;UAAErB,KAAK,EAALA,KAAK;UAAEsB,KAAK,EAAEhB;QAAG,CAAC,CAAC;MAC1C;IACF,CAAC,CAAC,CAAC,CAACiB,IAAI,CAAC,YAAM;MACb;MACAf,IAAI,EAAE;IACR,CAAC,CAAC,CAACgB,KAAK,CAAChB,IAAI,CAAC;EAChB,CAAC,CAAC;EACFV,GAAG,CAAC2B,MAAM,CAAC,MAAM,EAAE,IAAI,CAAC;AAC1B,CAAC,CAAC"}]}